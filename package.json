{
    "name": "path-tracker",
    "version": "1.2.0",
    "description": "Node.js library for dynamic and reliable file path tracking within complex project structures, overcoming traditional limitations.",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "scripts": {
        "format": "npx prettier --write 'src/**/*ts' 'index.ts' 'tests/**/*.ts'",
        "clean": "rm -r ./dist",
        "test": "npx jest",
        "build": "npx tsup index.ts --format cjs,esm --dts"
    },
    "keywords": [],
    "author": "AST-LW",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/AST-LW/path-tracker"
    },
    "dependencies": {
        "shelljs": "^0.8.5",
        "tsup": "^7.0.0",
        "typescript": "^5.1.3"
    },
    "devDependencies": {
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.2",
        "@types/shelljs": "^0.8.12",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1"
    }
}